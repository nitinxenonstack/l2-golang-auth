                                                <td scope="row">{{$i}}</td>
                                                <td>{{$nam.Title}}</td>
                                        
                                                <td>{{ $nam.Description }}</td>
                                                <td><a href='/article-edit-page/{{$nam.Slug}}'><i class="fa fa-pencil" aria-hidden="true"></i></a></td>
                                                <td> <button name = "slug" onclick="if (confirm('Delete this post?') == true){window.location.assign('/deletedoc/{{$nam.Slug}}')  }">Delete</button></a></td>
                                            </tr>
                                            {{end}}
                                          </tbody>
                                  </table>


